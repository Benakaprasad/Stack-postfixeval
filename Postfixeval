#include<stdio.h>
#include<stdlib.h>
#include<type.h>
#define max 100
int stack[max];
int top=-1;
void push(int ele)
{
if(top>=max-1)
{
printf("the stack is full");
}
else{
stack[++top]=ele;
printf("%d is pushed to stack",ele);
}
}
int pop()
{
if(top<0)
{
printf("the stack is empty");
}
else{
ele=stack[top--];
printf("%d is popped from the stack",ele);
}
return;
}
