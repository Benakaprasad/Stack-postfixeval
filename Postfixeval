#include<stdio.h>
#include<stdlib.h>
#include<type.h>
#define max 100
int stack[max];
int top=-1;
void push(int ele)
{
if(top>=max-1)
{
printf("the stack is full");
}
else{
stack[++top]=ele;
printf("%d is pushed to stack",ele);
}
}
int pop()
{
if(top<0)
{
printf("the stack is empty");
}
else{
ele=stack[top--];
printf("%d is popped from the stack",ele);
}
return;
}
int evalpostfix(char* postfix)
{
int op,i=0;
while(op=postfix[i++]!='\0')
{
if(isdigit(op)
{
push(op-'0');
}
else{
int op2=pop();
int op1=pop();
switch(op)
{
case '+':push(op1+op2);
break;
case '-':push(op1-op2);
break;
case '*':push(op1*op2);
break;
case '/':
if(op2!=0)
push(op1/op2);
break;
case '^':push(op1^op2);
break;
case '%':push(op1%op2);
break;
default:
printf("invalid choice");
return 0;
}
}
}
return pop();
}
int main()
{
char postfix[max];
printf("enter the postfix expression");
sacnf("%s",postfix);
int res=evalpostfix(postfix);
printf("value is:%d",res):
return 0;
}





